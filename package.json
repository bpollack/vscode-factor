{
    "name": "factor",
    "displayName": "Factor",
    "description": "Factor programming language",
    "version": "0.3.0",
    "publisher": "dexterhaslem",
    "icon": "factor_256x256.png",
    "homepage": "https://github.com/DexterHaslem/vscode-factor",
    "repository": {
        "type": "git",
        "url": "https://github.com/DexterHaslem/vscode-factor.git"
    },
    "bugs": {
        "url": "https://github.com/DexterHaslem/vscode-factor/issues"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "factor",
                "aliases": [
                    "Factor",
                    "factor"
                ],
                "extensions": [
                    ".factor",
                    ".facts"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "factor",
                "scopeName": "source.factor",
                "path": "./syntaxes/factor.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "factor",
                "path": "./snippets/snippets.json"
            }
        ]
    }
}
